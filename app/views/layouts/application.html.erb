<!DOCTYPE html>
<html>
  <head>
    <title>Proyectos</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render 'layouts/navbar' %>
    <% if notice.present? %>
      <p id="notice">
        <%= notice %>
      </p>
    <% end %>
    <% if alert.present? %>
      <p id="alert">
        <%= alert %>
      </p>
    <% end %>
    <%= yield %>
  </body>
</html>
